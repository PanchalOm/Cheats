pipeline{
  agent any
  tools { 
        maven 'maven' 
        }
  stages{
    stage('Build'){
      steps{
        sh "mvn clean"
        }
        }
    stage('SonarQube analysis'){
      steps{
        withSonarQubeEnv('sonarqube'){
          sh "mvn sonar:sonar"
          }
          }
          }
          }
          }
      
